" execute pathogen#infect()

syntax on

set sw=2
set ts=2
set expandtab

set ignorecase
set smartcase
set hlsearch
set incsearch

set listchars=tab:>-,trail:~,extends:>,precedes:<
set list

set bg=light

set number

" let vimclojure#ParenRainbow=1
" let vimclojure#HighlightBuiltins=1

hi Search ctermbg=LightBlue
